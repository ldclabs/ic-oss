{
  "name": "@ldclabs/ic_oss_ts",
  "version": "1.1.0",
  "type": "module",
  "description": "The Typescript version of the client SDK for the ic-oss cluster.",
  "license": "MIT",
  "homepage": "https://github.com/ldclabs/ic-oss",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ldclabs/ic-oss.git"
  },
  "engines": {
    "node": ">=18.16.0"
  },
  "browser": {
    "node:fs/promises": false,
    "node:path": false,
    "mime/lite": false
  },
  "files": [
    "candid",
    "dist",
    "src",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "prebuild": "dfx generate && rm -rf candid && cp -r '../declarations' './candid'",
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.{json,js,jsx,ts,tsx,css,scss}\"",
    "test": "vitest run",
    "ncu": "npx npm-check-updates -u"
  },
  "dependencies": {
    "@dfinity/agent": "^2.4.1",
    "@dfinity/candid": "^2.4.1",
    "@dfinity/principal": "^2.4.1",
    "@dfinity/utils": "^2.13.0",
    "@noble/hashes": "^1.8.0",
    "mime": "^4.0.7",
    "web-streams-polyfill": "^4.1.0"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/node": "22.15.3",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2"
  }
}